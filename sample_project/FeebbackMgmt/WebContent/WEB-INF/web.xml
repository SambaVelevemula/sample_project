<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>FeedBack Management</display-name>
  <welcome-file-list>
    <welcome-file>customerServiceForm.jsp</welcome-file>
  </welcome-file-list>
   
  <error-page>  
  <error-code>405</error-code>  
  <location>/ShowError.jsp</location>  
  </error-page>
  
  <servlet>
    <servlet-name>CustomerServiceController</servlet-name>
    <servlet-class>com.controller.CustomerServiceController</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CustomerServiceController</servlet-name>
    <url-pattern>/CustomerServiceController</url-pattern>
  </servlet-mapping>
    
</web-app>